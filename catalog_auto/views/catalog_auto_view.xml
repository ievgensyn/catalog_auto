<odoo>
    <record id="catalog_auto_model_list_view" model="ir.ui.view">
        <field name="name">catalog.auto.model</field>
        <field name="model">catalog.auto.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="brand_id"/>
            </tree>
        </field>
    </record>
    <record id="catalog_auto_model_form_view" model="ir.ui.view">
        <field name="name">catalog.auto.model.form.view</field>
        <field name="model">catalog.auto.model</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="brand_id"/>
                            <field name="vin_ids"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_catalog_auto_model" model="ir.actions.act_window">
        <field name="name">Model</field>
        <field name="res_model">catalog.auto.model</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">Create the first Model</p>
            </field>
    </record>

